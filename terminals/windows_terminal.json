{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions":
    [
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "up"
            },
            "keys": "ctrl+shift+k"
        },
        {
            "command":
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command":
            {
                "action": "splitPane",
                "split": "right"
            },
            "keys": "ctrl+\\"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "down"
            },
            "keys": "ctrl+shift+j"
        },
        {
            "command":
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": "unbound",
            "keys": "alt+down"
        },
        {
            "command": "unbound",
            "keys": "alt+left"
        },
        {
            "command": "unbound",
            "keys": "alt+up"
        },
        {
            "command": "unbound",
            "keys": "alt+shift+plus"
        },
        {
            "command": "unbound",
            "keys": "win+sc(41)"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "right"
            },
            "keys": "ctrl+shift+l"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "left"
            },
            "keys": "ctrl+shift+h"
        },
        {
            "command": "togglePaneZoom",
            "keys": "ctrl+shift+o"
        },
        {
            "command":
            {
                "action": "quakeMode"
            },
            "keys": "win+shift+q"
        },
        {
            "command":
            {
                "action": "moveTab",
                "direction": "backward"
            },
            "keys": "ctrl+shift+["
        },
        {
            "command":
            {
                "action": "moveTab",
                "direction": "forward"
            },
            "keys": "ctrl+shift+]"
        }
    ],
